FoamFile
{
	version		4.0;
	format		ascii;
	class		volScalarField;
	location	"0";
	object		epsilon;
}

dimensions		[0 2 -3 0 0 0 0];

internalField		uniform 0.01;

boundaryField
{

    boundingbox_empty
    {

        type		empty;

    }

    west_window
    {

        type		fixedValue;

        value		uniform 0.01;

    }

    room
    {

        type		epsilonWallFunction;

        value		uniform 0.01;

    }

    east_window
    {

        type		zeroGradient;

    }

}