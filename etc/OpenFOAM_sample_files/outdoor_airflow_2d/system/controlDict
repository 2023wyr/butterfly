FoamFile
{
	version		4.0;
	format		ascii;
	class		dictionary;
	location	"system";
	object		controlDict;
}

#include		"probes";

application		simpleFoam;

startFrom		latestTime;

startTime		0;

stopAt		endTime;

endTime		5000;

deltaT		1;

writeControl		timeStep;

writeInterval		100;

purgeWrite		0;

writeFormat		ascii;

writePrecision		7;

writeCompression		off;

timeFormat		general;

timePrecision		6;

runTimeModifiable		true;

