FoamFile
{
	version		4.0;
	format		ascii;
	class		volVectorField;
	location	"0";
	object		U;
}

dimensions		[0 1 -1 0 0 0 0];

internalField		uniform (0 0 0);

boundaryField
{

    room
    {

        type		fixedValue;

        value		uniform (0 0 0);

    }

    floor
    {

        type		fixedValue;

        value		uniform (0 0 0);

    }

    roof
    {

        type		fixedValue;

        value		uniform (0 0 0);

    }

}